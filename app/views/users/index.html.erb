<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>


<table>
	<th> ID </th> <th> Name </th> <th> Email </th>
	<% @users.each do |user| %>
	<tr>
		<td>
			<%= user.id %>
		</td>
		<td>
			<%= user.username %>
		</td>
		<td>
			<%= user.email %>
		</td>
	</tr>
	<% end %>
</table>
<hr>
<%= link_to "Create", new_user_path %>
